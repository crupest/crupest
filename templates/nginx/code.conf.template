server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name code.${CRUPEST_DOMAIN};
    
    location / {
        include common/proxy-common;
        proxy_pass http://debian-dev:8080/;
    }

    client_max_body_size 5G;
}


server {
    listen 80;
    listen [::]:80;
    server_name code.${CRUPEST_DOMAIN};

    include common/https-redirect;
    include common/acme-challenge;
}
