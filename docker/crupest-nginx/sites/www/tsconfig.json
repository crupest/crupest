{
    "extends": "@tsconfig/recommended/tsconfig.json",
    "compilerOptions": {
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "types": [
            "parcel-env"
        ],
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true
    }
}